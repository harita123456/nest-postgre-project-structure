{
  "name": "project-structure-socket",
  "version": "0.0.1",
  "description": "",
  "author": "Jignesh Karena",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "preinstall": "pnpm install project-structure-database",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:setup": "mkdir -p drizzle/meta && New-Item -Path drizzle/meta/_journal.json -ItemType File || echo '{}' > drizzle/meta/_journal.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:gen-and-migrate": "drizzle-kit generate && drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "app": "run-s lint format build start:prod",
    "dev": "pnpm run start:dev",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/client-secrets-manager": "^3.925.0",
    "@aws-sdk/s3-request-presigner": "^3.910.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@sentry/nestjs": "^10.17.0",
    "async-retry": "^1.3.3",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "firebase-admin": "^13.5.0",
    "googleapis": "^161.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nestjs-i18n": "^10.5.1",
    "openai": "^6.7.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "project-structure-database": "file:..\\project-structure-database",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "4.8.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/async-retry": "^1.4.9",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}