# -----------------------
# Socket
# -----------------------
NODE_ENV=development
PORT=9006

# Host binding for Nest app
HOST=0.0.0.0

# Comma-separated allowed origins for CORS (no spaces). Example:
# http://localhost:3000,https://staging.yourapp.com
CORS_ALLOWED_ORIGINS=http://localhost:3000

# Observability / Environment label for Sentry etc. (development|staging|production)
SERVER_ENVIRONMENT=project-structure-socket-dev

# -----------------------
# Database
# -----------------------
DB_HOSTNAME=
DB_PORT=5432
DB_USERNAME=
DB_PASSWORD=
DB_NAME=
DB_SSL=false

# -----------------------
# JWT
# -----------------------
TOKEN_KEY=
TOKEN_EXPIRES_IN=1d
TOKEN_ISSUER=
TOKEN_AUDIENCE=
REFRESH_TOKEN_TTL_DAYS=30

# -----------------------
# AWS / S3 (bucket.config.ts / aws-s3.util.ts)
# -----------------------
REGION=
BUCKET_NAME=       # prefix used in S3 keys, e.g. dev/ or prod/
S3_SIGNED_URL_TTL_SECONDS=86400         # 1 day
BUCKET_URL = "www.google.com"

# -----------------------
# Sentry (utils/sentry.ts, utils/logger.ts)
# -----------------------
# SERVER_ENVIRONMENT=project-structure-socket-dev
# SENTRY_DSN=https://9af0db4878eaed47187568bd37a2e865@o4509450179903488.ingest.de.sentry.io/4509954820276304  #kp
# SENTRY_LOG_LEVEL=all   # one of: error|warn|info|debug|trace|fatal|all
# SENTRY_WS_LOG_EVENTS=true
# SENTRY_WS_TRACE=true
# SENTRY_SEND_DEFAULT_PII=false
# SENTRY_LOG_LEVEL=all

# -----------------------
# Firebase/FCM (notification.service.ts)
# Provide credentials via env (no file dependency)
# If FIREBASE_PROJECT_ID is not set, PROJECT_ID will be used as a fallback
# For FIREBASE_PRIVATE_KEY, escape newlines as \n when storing in .env
# -----------------------
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY=""

# -----------------------
# i18n
# -----------------------
I18N_DEFAULT_NS=common
SECRET_NAME=ProjectStructureSecrets